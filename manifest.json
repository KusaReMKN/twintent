{
	"manifest_version": 3,
	"name": "twintent",
	"version": "0.2.0",
	"description": "訪問しているページを共有しなさい‼️💢",
	"action": {
		"default_popup": "index.html"
	},
	"permissions": [
		"activeTab",
		"storage"
	],
	"icons": {
		"32": "./icons/icon32.png",
		"48": "./icons/icon48.png",
		"96": "./icons/icon96.png",
		"128": "./icons/icon128.png"
	},
	"browser_specific_settings": {
		"gecko": {
			"id": "twintent@kusaremkn.com"
		}
	}
}
